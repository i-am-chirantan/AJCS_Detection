{
   "builds": [
      {
         "src": "./AJCS_Detection/wsgi.py",
         "use": "@vercel/python",
         "config": {
            "maxLambdaSize": "100mb",
            "python": "3.10.10"
         }
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "AJCS_Detection/wsgi.py"
      }
   ]
}